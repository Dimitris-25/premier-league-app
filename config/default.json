{
  "authentication": {
    "secret": "${JWT_SECRET}",
    "entity": "user",
    "service": "users",
    "authStrategies": ["jwt", "local"],
    "jwtOptions": { "expiresIn": "1d" },
    "local": { "usernameField": "email", "passwordField": "password" }
  },

  "seedVenuesOnBoot": false,
  "port": 3030
}
